<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="abstractUserService" class="com.example.service.abstracrService.AbstractService">
        <constructor-arg name="repository" ref="userDao"/>
    </bean>

    <bean id="abstractUserServiceProp" class="com.example.service.abstracrService.AbstractService">
        <constructor-arg name="repository" ref="propertiesDao"/>
    </bean>
</beans>